{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SkatteverketAPI": {
    "BaseUrl": "https://app.skatteverket.se/momsdeklaration/v1",
    "TestBaseUrl": "https://test.app.skatteverket.se/momsdeklaration/v1",
    "UseTestEnvironment": true,
    "OAuth": {
      "ClientId": "",
      "ClientSecret": "",
      "TokenEndpoint": "https://orgoauth2.skatteverket.se/oauth2/v1/org/token",
      "TestTokenEndpoint": "https://test-orgoauth2.skatteverket.se/oauth2/v1/org/token",
      "AuthorizationEndpoint": "https://orgoauth2.skatteverket.se/oauth2/v1/org/authorize",
      "TestAuthorizationEndpoint": "https://test-orgoauth2.skatteverket.se/oauth2/v1/org/authorize",
      "Scope": "momsdeklaration:read momsdeklaration:write",
      "GrantType": "client_credentials"
    },
    "Certificate": {
      "Path": "",
      "Password": "",
      "Thumbprint": "",
      "StoreName": "My",
      "StoreLocation": "CurrentUser",
      "ValidationMode": "ChainTrust"
    },
    "Timeout": 30,
    "RetryCount": 3,
    "RetryDelaySeconds": 2
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/momsdeklaration-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{SourceContext}] {Message:lj} {Properties:j}{NewLine}{Exception}",
          "retainedFileCountLimit": 30
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}